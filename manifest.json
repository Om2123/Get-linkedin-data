{
  "manifest_version": 3,
  "name": "Get linkedin Profile Data",
  "version": "1.0",
  "description": "A Chrome extension to get the data and store it using sqlite ",
  "permissions": ["activeTab",
   "scripting" ,
   "https://www.linkedin.com/in/rajesh-kr-mehra/" , 
   "https://www.linkedin.com/*"] ,
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
 
  
}
