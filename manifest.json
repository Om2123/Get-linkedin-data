{
  "manifest_version": 3,
  "name": "LinkedIn Extension",
  "version": "1.0",
  "description": "A Chrome extension to get the data and store it using sqlite ",
  "permissions": ["activeTab",
   "scripting" ] ,
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
 
  
}
